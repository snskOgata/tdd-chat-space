.form
  = form_with model: @group do |f|
    .field
      .fiels__label
        = f.label :name
      .field_input
        = f.text_field :name
    .field
      .field__label
        = f.label :user_ids
      .field__input
        = f.collection_check_boxes :user_ids, User.all, :id, :name
    .field
      .field__action
        = f.submit
